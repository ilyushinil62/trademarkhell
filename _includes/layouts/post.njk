eleventyNavigation:
key: post

---

<!DOCTYPE html>
<html>

<head>
  <link href="/css/post.css" rel="stylesheet" />
</head>

<body>
  <div class="container">
    <div class="header">
      <ul class="headertop">
        <p>nav</p>
        <li class="headertoptext">
          <b>trademarkhell.net // trademark's special hell</b>
        </li>
      </ul>
      <ul class="header-flex">
        <ul class="nav">
          <li class="navitem">
           <p><a href="/index" l>home</a></p>
          </li>
          <li class="navitem">
            <p><a href="/tags/blog/" l>blog</a></p>
          </li>
          <li class="navitem">
            <p><a href="/tags/tech/" l>tech</a></p>
          </li>
          <li class="navitem">
            <p><a href="/tags/music/" l>music</a></p>
          </li>
          <li class="navitem">
            <p><a href="/tags/anime/" l>anime</a></p>
          </li>
          <li class="navitem">
            <p><a href="/tags/nil/" l>soon</a></p>
          </li>
          <li class="navitem">
            <p><a href="/tags/nil/" l>soon</a></p>
          </li>
          <li class="navitem">
            <p><a href="/tags/nil/" l>soon</a></p>
          </li>
        </ul>
        <li class="banner"></li>
      </ul>
    </div>
    <div class="content">
      <ul class="maintext">
        <li class="maintexttop">
          <p> <a href="javascript:history.back()">back</a>
            // {{title}} // {{ page.date | readableDate }} </p>
        </li>
        <ul class="maintextcontent">

          {{ content | safe }}
          {%- if collections.posts %}
          {%- set previousPost = collections.posts | getPreviousCollectionItem %}
          {%- set nextPost = collections.posts | getNextCollectionItem %}
          {%- if nextPost or previousPost %}

          {%- endif %}
          {%- endif %}
          <ul class="post-metadata">
            {%- for tag in tags | filterTagList %}
            {%- set tagUrl %}/tags/{{ tag | slugify }}/{% endset %}
            {%- endfor %}

          </ul>
          <li class="posttexttop">
            <p>
              {%- if previousPost %}<b>  <a href="{{ previousPost.url }}">previous</a></b>
               //
              {% endif %}
              comments 
               //  
              {%- if nextPost %}<b><a href=" {{ nextPost.url }}"> next</a></b>{% endif %}
            </p>
          </li>
          <script src="https://utteranc.es/client.js" repo="ilyushinil62/trademarkhell" issue-term="pathname"
            theme="photon-dark" crossorigin="anonymous" async>
            </script>
        </ul>
      </ul>

      <div class="footer">
        <p>Hosted on Nekoweb // Powered by 11ty // All Rights Reserved</p>
      </div>


</body>
<style>
  body {
    margin: auto;
    display: block;
    width: 95%;
    height: 95%;
    background-image: url("https://trademarkhell.net/checker25.png");
    animation: bgScroll 35s linear infinite;
  }
</style>

</html>